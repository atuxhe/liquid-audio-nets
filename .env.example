# Liquid Audio Nets Environment Configuration
# Copy this file to .env and customize for your environment

# Development Environment
NODE_ENV=development
RUST_LOG=debug
RUST_BACKTRACE=1
PYTHONPATH=./python

# Audio Processing
AUDIO_SAMPLE_RATE=16000
AUDIO_BUFFER_SIZE=512
AUDIO_INPUT_DEVICE=default
AUDIO_OUTPUT_DEVICE=default

# Model Configuration
MODEL_CACHE_DIR=./models
MODEL_DEFAULT_PRECISION=f32
MODEL_QUANTIZATION=int8
MODEL_MAX_SIZE_MB=10

# Training Configuration
TRAINING_DATA_DIR=./data
TRAINING_BATCH_SIZE=32
TRAINING_LEARNING_RATE=0.001
TRAINING_MAX_EPOCHS=100
TRAINING_CHECKPOINT_DIR=./checkpoints

# Hardware Testing
HARDWARE_TARGET_PLATFORM=stm32f407
HARDWARE_SERIAL_PORT=/dev/ttyUSB0
HARDWARE_FLASH_ADDRESS=0x08000000
HARDWARE_RAM_SIZE=192KB

# Embedded Deployment
EMBEDDED_OPTIMIZATION_LEVEL=3
EMBEDDED_ENABLE_FPU=true
EMBEDDED_STACK_SIZE=32KB
EMBEDDED_HEAP_SIZE=16KB

# Power Management
POWER_BUDGET_MW=2.0
POWER_MEASUREMENT_ENABLED=false
POWER_PROFILING_INTERVAL=100

# Security
SECURITY_MODEL_ENCRYPTION=false
SECURITY_INPUT_VALIDATION=true
SECURITY_AUDIT_LOGGING=false

# Monitoring and Observability
MONITORING_ENABLED=false
METRICS_EXPORT_INTERVAL=60
METRICS_PROMETHEUS_PORT=9090
LOGGING_LEVEL=info
LOGGING_FORMAT=json

# Testing
TEST_AUDIO_SAMPLES_DIR=./test_data
TEST_HARDWARE_AVAILABLE=false
TEST_SKIP_SLOW=false
TEST_COVERAGE_THRESHOLD=80

# Documentation
DOCS_BUILD_DIR=./docs/build
DOCS_PORT=8000
DOCS_AUTO_RELOAD=true

# Cloud Integration (Optional)
# AWS_ACCESS_KEY_ID=your_aws_key
# AWS_SECRET_ACCESS_KEY=your_aws_secret
# AWS_DEFAULT_REGION=us-west-2
# S3_MODEL_BUCKET=liquid-audio-models

# Development Tools
DEV_ENABLE_PROFILER=false
DEV_ENABLE_DEBUGGER=false
DEV_AUTO_RELOAD=true
DEV_HOT_RELOAD=false

# CI/CD Configuration
CI_SKIP_SLOW_TESTS=false
CI_PARALLEL_JOBS=4
CI_CACHE_ENABLED=true